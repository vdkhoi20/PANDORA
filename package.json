{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "export": "next export",
    "pages:build": "next build && next export -o docs",
    "pages:docs": "npm run build && rm -rf docs && mkdir -p docs/_next && mkdir -p docs/_next/static && cp -R .next/static/* docs/_next/static/ && cp -R .next/server/app/* docs/ || true && cp -R public/* docs/ || true && touch docs/.nojekyll"
  },
  "dependencies": {
    "next": "15.5.9",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.9",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
